<%- include("partials/header"); -%>
<div class="content">
  <h1>Home</h1>
  <p><%= startingContent %></p>

  <% posts.forEach(function(post){ %>

  <h1><%=post.title%></h1>
  <p>
    <%=post.content.substring(0, 200) + " ..."%>
    <a href="/posts/<%=post.title%>">Read More</a>
  </p>

  <% }) %>
</div>
